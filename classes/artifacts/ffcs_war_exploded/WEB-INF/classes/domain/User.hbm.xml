<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ffcs.xkjs.domain.User" table="xkjs_user">

        <id name="userId" type="string" column="userId">
            <generator class="uuid"></generator>
        </id>


        <property name="userName" type="java.lang.String">
            <column name="userName" length="60" />
        </property>

        <property name="password" type="java.lang.String">
            <column name="password" length="60" />
        </property>

        <property name="role" type="java.lang.String">
            <column name="role" length="25" />
        </property>


        <property name="sno" type="java.lang.String">
            <column name="sno" length="25" />
        </property>

        <property name="grade" type="java.lang.String">
            <column name="grade" length="25" />
        </property>

        <property name="academe" type="java.lang.String">
            <column name="academe" length="60" />
        </property>

        <property name="profession" type="java.lang.String">
            <column name="profession" length="60" />
        </property>

        <property name="classes" type="java.lang.String">
            <column name="classes" length="25" />
        </property>


        <property name="trueName" type="java.lang.String">
            <column name="trueName" length="25" />
        </property>

        <property name="telephone" type="java.lang.String">
            <column name="telephone" length="60" />
        </property>

        <property name="email" type="java.lang.String">
            <column name="email" length="60" />
        </property>



    </class>
</hibernate-mapping>