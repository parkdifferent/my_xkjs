<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ffcs.xkjs.domain.Result" table="xkjs_result">

        <id name="resultId" type="string" column="resultId">
            <generator class="uuid"></generator>
        </id>

        <property name="sno" type="java.lang.String">
            <column name="sno" length="60" />
        </property>

        <property name="grade" type="java.lang.String">
            <column name="grade" length="25" />
        </property>

        <property name="academe" type="java.lang.String">
            <column name="academe" length="60" />
        </property>

        <property name="profession" type="java.lang.String">
            <column name="profession" length="60" />
        </property>

        <property name="classes" type="java.lang.String">
            <column name="classes" length="30" />
        </property>

        <property name="name" type="java.lang.String">
            <column name="name" length="25" />
        </property>

        <property name="comName" type="java.lang.String">
            <column name="comName" length="60" />
        </property>

        <property name="form" type="java.lang.String">
            <column name="form" length="25" />
        </property>

        <property name="prize" type="java.lang.String">
            <column name="prize" length="60" />
        </property>

        <property name="tutor" type="java.lang.String">
            <column name="tutor" length="25" />
        </property>

        <property name="getTime" type="date" column="getTime"></property>


    </class>
</hibernate-mapping>
